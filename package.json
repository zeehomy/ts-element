{
  "name": "ts-element",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css",
    "./src/**/*.css"
  ],
  "main": "./dist/umd/ts-element.umd.cjs",
  "module": "./dist/es/ts-element.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/ts-element.js",
      "require": "./dist/umd/ts-element.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/es/index.css",
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build-native": "run-p type-check \"build-only {@}\" --",
    "build0.9": "run-p build-umd build-es && npm run move-style",
    "build": "npm run build-only && npm run move-style",
    "preview": "vite preview",
    "build-only0.9": "vite build",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "move-style": "move-file dist/es/index.css dist/index.css"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "move-file-cli": "^3.0.0",
    "npm-run-all2": "^6.2.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "typescript": "~5.4.0",
    "unplugin-vue-macros": "^2.9.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue": "^3.4.29",
    "vue-tsc": "^2.0.21"
  }
}
